{
  "name": "jcpunk-rhsm_only",
  "version": "0.1.6",
  "author": "Pat Riehecky <riehecky@fnal.gov>",
  "summary": "Ensures the only file in /etc/yum.repos.d/ is redhat.repo",
  "license": "GPL-3.0+",
  "source": "https://github.com/jcpunk/puppet-rhsm_only",
  "project_page": "https://github.com/jcpunk/puppet-rhsm_only",
  "issues_url": "https://github.com/jcpunk/puppet-rhsm_only/issues",
  "tags": ["subscription manager", "rhsm"],
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_range": ">= 2.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [ "5", "6", "7" ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [ "5", "6", "7" ]
    },
    {
      "operatingsystem": "OracleLinux",
      "operatingsystemrelease": [ "5", "6", "7" ]
    },
    {
      "operatingsystem": "Scientific",
      "operatingsystemrelease": [ "5", "6", "7" ]
    }
  ]
}

